					<url>http://10.232.225.42:8081/nexus/content/repositories/snapshots-tag-usu</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>
		<profile>
			<id>nexus-tag-pro</id>
			<activation>
				<activeByDefault>false</activeByDefault>
				<file>
				  <exists>${basedir}/../.tag-pro</exists>
