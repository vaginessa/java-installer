					<url>http://10.232.225.42:8081/nexus/content/repositories/snapshots-usu</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>
		<profile>
			<id>nexus-pro</id>
			<activation>
				<activeByDefault>false</activeByDefault>
				<file>
				  <exists>${basedir}/../.pro</exists>
